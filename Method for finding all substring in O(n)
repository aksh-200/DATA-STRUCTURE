  int i =0;
        int j= i+1;
        
        int count = 0;
        while(i < str.length()-1)
        {
            if(str.charAt(i) == '1' && str.charAt(j)=='1')
            {
                count++;
            }
            j++;
        
            if(j == str.length())
            {   
                i++;
                j=i+1;
            }
            

        }
